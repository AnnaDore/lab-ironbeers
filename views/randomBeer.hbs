<div class='random-page'>
    {{#each randBeer}}
    <div class="random-text">
        {{>beerpartial}}
        <a href="/{{id}}">
            <h4><b>Food pairing:</b></h4>
           <ul>
               {{#each food_pairing}}
                <li>{{this}}</li>
                {{/each}}
           </ul>
            <p><b>Brewer Tips: </b>{{brewers_tips}}</p>
        </a>
    </div>
    <div class="random-beer-image">
        {{>imagepartial}}
    </div>
    {{/each}}
</div>